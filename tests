import unittest

class TestRhythmGeneration(unittest.TestCase):
    def test_generate_rhythm(self):
        # SimulaciÃ³n basada en el backend
        def generate_rhythm(tempo, beats):
            return f"Ritmo a {tempo} BPM con {beats} beats: [{'ta ' * beats}]"
        result = generate_rhythm(120, 4)
        expected = "Ritmo a 120 BPM con 4 beats: [ta ta ta ta ]"
        self.assertEqual(result.strip(), expected.strip())

if __name__ == "__main__":
    unittest.main()
